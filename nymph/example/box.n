#include <stdlib.h>
#include <stdio.h>

pub double cm_to_inches(double cm) {
  return cm / 2.54;
}

obj Box {
  int width = 1;
  int height = 1;
  int depth = 1;
  double (*func1)(double) = cm_to_inches;
}

int add(int a, int b) {
  return a + b;
}

pub void printBox(Box *this) {
    printf("%i\n", add(this->width, this->height));
}
